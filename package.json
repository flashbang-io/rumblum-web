{
  "name": "rumblum-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy:sandbox": "REACT_APP_NODE_ENV=sandbox yarn run build && now ./build --name=rumblum-web --cache=60000 --static && now alias --local-config=now.sandbox.json",
    "deploy:production": "REACT_APP_NODE_ENV=production yarn run build && now ./build --name=rumblum-web --cache=60000 --static && now alias --local-config=now.production.json",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "change-case": "^3.0.1",
    "chroma-js": "^1.3.5",
    "http-status": "^1.0.1",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "raven-for-redux": "^1.2.0",
    "raven-js": "^3.21.0",
    "react": "^16.2.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-onclickoutside": "^6.7.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-stripe-elements": "^1.3.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-auth-wrapper": "^2.0.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.2.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1"
  }
}
